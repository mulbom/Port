#include <iostream>

using namespace std;

void SystemDefault() {
	cin.tie(NULL);
	ios_base::sync_with_stdio(false);
}

int gcd(int a, int b) {
	int r = a % b;
	if (r == 0) {
		return b;
	}
	else {
		return gcd(b, r);
	}
}

int main() {	
	SystemDefault();
	
	int a, b, c, d;
	cin >> a >> b >> c >> d;
	int e = (a * d) + (c * b);
	int f = (b * d);
	int x = gcd(e, f);

	cout << (e / x) << " " << (f / x);

	return 0;
}

// 공배수 활용
