#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

typedef pair<int, int> a;

bool compare(a n1, a n2) {
    if (n1.second == n2.second) {
        return n1.first < n2.first;
    }
    return n1.second < n2.second;
}
int main() {
    int n;
    cin >> n;
    vector<a> s(n);
    for (int i = 0; i < n; i++) {
        cin >> s[i].first >> s[i].second;
    }
    sort(s.begin(), s.end(), compare);

    for (int i = 0; i < n; i++) {
        cout << s[i].first << " " << s[i].second << "\n";
    }

    return 0;
}
//compare함수에서 기본적으로 y값을 기준으로 정렬
//y값이 같으면 x값을 기준으로 정렬하도록 설정
