#include <iostream>
#include <set>

using namespace std;

void SystemDefault() {
	cin.tie(NULL);
	ios_base::sync_with_stdio(false);
}

int main() {
	SystemDefault();

	int a, b, n;
	cin >> a >> b;
	set<int> d;
	for (int i = 0; i < a; i++) {
		cin >> n;
		d.insert(n);
	}
	for (int i = 0; i < b; i++) {
		cin >> n;
		if (d.find(n) == d.end()) {
			d.insert(n);
		}
		else {
			d.erase(n);
		}
	}
	cout << d.size() << "\n";
	return 0;
}
