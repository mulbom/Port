#include <iostream>
#include <set>
#include <string>

using namespace std;

void SystemDefault() {
	cin.tie(NULL);
	ios_base::sync_with_stdio(false);
}

int main() {
	SystemDefault();

	string s, a;
	cin >> s;
	
	set<string> ss;
	
	for (int i = 0; i < s.length(); i++) {
		a = "";
		for (int j = i; j < s.length(); j++) {
			a += s[j];
			ss.insert(a);
		}
	}
	
	cout << ss.size() << "\n";
	
	return 0;
}
